{
	"version": "0.2",
	"language": "en",
	"ignorePaths": [
		"vite.config.ts.timestamp-*",
		"pnpm-lock.yaml",
		"build",
		"dist"
	],
	"ignoreRegExpList": [
		"/\"data\\:image\\/svg\\+xml.*?\"/g"
	],
	"words": [
		"neomorphism",

		/* Packages */
		"tabler",
		"tobper",
		"vite"
	],
	"overrides": [
		{
			"filename": "package.json",
			"words": [ "persson", "preinstall", "publint", "tsbuildinfo" ]
		},
		{
			"filename": "LICENSE",
			"words": [ "persson" ]
		},
		{
			"filename": "*.css",
			"words": [
				"combobox",
				"datetime"
			]
		},
		{
			"filename": "*.svelte",
			"words": [
				"onclickcapture"
			]
		},
		{
			"filename": "*.svg",
			"words": [
				"evenodd"
			]
		}
	],
	"enabled": true
}
